{
  "name": "@stripped-ui/eslint-config-ts",
  "version": "0.1.0-beta.0",
  "description": "ESLint shareable config for TypeScript used in @stripped-ui",
  "main": ".eslintrc.js",
  "files": [
    ".eslintrc.js"
  ],
  "scripts": {
    "test": "jest",
    "check-valid": "eslint tests/fixtures/*.valid.ts --config .eslintrc.tests.js",
    "check-invalid": "eslint tests/fixtures/*.invalid.ts --config .eslintrc.tests.js"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Oleg Dutchenko",
    "email": "dutchenko.o.dev@gmail.com"
  },
  "keywords": [
    "@stripped-ui",
    "code-style",
    "eslint",
    "eslintconfig",
    "ts",
    "typescript"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@stripped-ui/eslint-config": "^0.0.10-beta.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.11.0",
    "jest": "^26.6.0",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "eslint": "^7.11.0"
  }
}
